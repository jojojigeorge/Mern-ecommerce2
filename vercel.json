{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "redirects": [
        {
            "source": "/getproduct-photo",
            "destination": "https://server-mern-ecommerce2.vercel.app"
        }
    ]
}